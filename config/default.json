{
  "port": 8048,

  "builder-config-name": "builder-config.json",
  "check-jobpool-results-only": false,
  "keep-legacy-results": true,
  "output-dir": "/usr/build-result/",
  "temp-dir": "%SYSTEM_TEMP%/extension-builder/",
  "workspace": "/usr/workspace/",
  "server-status-page-enabled": true,
  "server-status-api-enabled": true,

  "build-with-github-repo-enabled": true,
  "build-with-zip-enabled": true,
  "build-with-plain-source-enabled": false,

  "whitelist-enabled": false,
  "whitelist": [
    {
      "owner": "OpenSourceAIX",
      "repo": "ColinTreeListView",
      "refs": "master"
    }
  ],

  "github-auth-type": "none"
}
